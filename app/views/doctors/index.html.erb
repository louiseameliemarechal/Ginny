<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>

<% @doctors.each do |doctor| %>
  <p><%= "#{doctor.first_name.capitalize} #{doctor.last_name.capitalize}" %></p>
  <p><%= "#{doctor.street}" %></p>
  <p><%= "#{doctor.city}" %></p>
  <p><%= "#{doctor.country}" %></p>
<% end %>
